{
  "name": "WDIO-E2E-TESTING",
  "type": "module",
  "devDependencies": {
    "@types/supertest": "^6.0.2",
    "@wdio/allure-reporter": "^8.39.0",
    "@wdio/cli": "^8.39.1",
    "@wdio/cucumber-framework": "^8.39.0",
    "@wdio/local-runner": "^8.39.1",
    "@wdio/spec-reporter": "^8.39.0",
    "allure-commandline": "^2.29.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "wdio": "wdio run ./wdio.conf.ts",
    "report-open": "allure open allure-report",
    "report-generate": "allure generate --clean allure-results",
    "pretest": "rimraf allure-results && rimraf allure-report && rimraf test-screenshots && rimraf wdio-logs",
    "test": "wdio run ./wdio.conf.ts --cucumberOpts.tags='@Flipkart'"
